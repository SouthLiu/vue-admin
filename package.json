    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.26",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-theme": "^0.8.1",
    "vue-tsc": "^0.29.8"
